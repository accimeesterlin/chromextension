{
    "name": "Tutor Chrome Extension",
    "version": "1.2",
    "description": "Build a basic extension",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Tutor App"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["moment.js", "jquery.js", "autoprefill.js"]
    }]
}