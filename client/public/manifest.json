{
    "name": "Tutor Chrome Extension",
    "version": "1.0",
    "description": "Build a basic extension",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "index.html"
    }
}